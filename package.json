{
  "name": "nuxt-framework",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "FORCE_COLOR=1 pnpm --filter './packages/**' prepack",
    "changelog": "changelogen",
    "cleanup": "rimraf 'packages/**/node_modules' 'examples/**/node_modules' 'node_modules'",
    "dev": "pnpm nuxi dev playground",
    "dev:build": "pnpm nuxi build playground",
    "dev:preview": "pnpm nuxi preview playground",
    "example": "./scripts/example.sh dev",
    "example:build": "./scripts/example.sh build",
    "lint": "eslint --ext .vue,.ts,.js,.mjs .",
    "lint:docs": "markdownlint ./docs/content && case-police 'docs/content**/*.md'",
    "lint:docs:fix": "markdownlint ./docs/content --fix && case-police 'docs/content**/*.md' --fix",
    "nuxi": "cross-env NUXT_TELEMETRY_DISABLED=1 JITI_ESM_RESOLVE=1 nuxi",
    "nuxt": "cross-env NUXT_TELEMETRY_DISABLED=1 JITI_ESM_RESOLVE=1 nuxi",
    "play": "echo use pnpm dev && exit 1",
    "release": "pnpm i && pnpm lint && FORCE_COLOR=1 lerna publish -m \"chore: release\" && pnpm stub",
    "stub": "pnpm --filter './packages/**' prepack --stub",
    "test:fixtures": "cross-env NUXT_TELEMETRY_DISABLED=1 pnpm nuxi prepare test/fixtures/basic && JITI_ESM_RESOLVE=1 vitest run --dir test",
    "test:fixtures:dev": "cross-env NUXT_TELEMETRY_DISABLED=1 NUXT_TEST_DEV=true pnpm test:fixtures",
    "test:fixtures:webpack": "cross-env NUXT_TELEMETRY_DISABLED=1 TEST_WITH_WEBPACK=1 pnpm test:fixtures",
    "test:types": "pnpm nuxi prepare test/fixtures/basic && cd test/fixtures/basic && npx vue-tsc --noEmit",
    "test:unit": "cross-env JITI_ESM_RESOLVE=1 vitest run --dir packages",
    "typecheck": "tsc --noEmit",
    "version": "pnpm i && git add pnpm-lock.yaml"
  },
  "resolutions": {
    "@nuxt/kit": "workspace:*",
    "@nuxt/schema": "workspace:*",
    "@nuxt/test-utils": "workspace:*",
    "@nuxt/vite-builder": "workspace:*",
    "@nuxt/webpack-builder": "workspace:*",
    "nuxi": "workspace:*",
    "nuxt": "workspace:*",
    "nuxt3": "workspace:nuxt@*",
    "vue": "3.2.39",
    "vite": "~3.1.3",
    "unbuild": "^0.8.11"
  },
  "devDependencies": {
    "@actions/core": "^1.9.1",
    "@nuxt/kit": "workspace:*",
    "@nuxt/schema": "workspace:*",
    "@nuxt/test-utils": "workspace:*",
    "@nuxt/vite-builder": "workspace:*",
    "@nuxt/webpack-builder": "workspace:*",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@types/crawler": "^1.2.2",
    "@types/node": "^16.11.60",
    "@types/rimraf": "^3",
    "@unocss/reset": "^0.45.23",
    "case-police": "^0.5.10",
    "changelogen": "^0.3.2",
    "crawler": "^1.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "execa": "^6.1.0",
    "expect-type": "^0.14.2",
    "globby": "^13.1.2",
    "jiti": "^1.16.0",
    "lerna": "^5.5.2",
    "markdownlint-cli": "^0.32.2",
    "nuxi": "workspace:*",
    "nuxt": "workspace:*",
    "pathe": "^0.3.8",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vitest": "~0.19.1",
    "vue-tsc": "^0.39.5"
  },
  "packageManager": "pnpm@7.12.0",
  "engines": {
    "node": "^14.16.0 || ^16.11.0 || ^17.0.0 || ^18.0.0"
  }
}
